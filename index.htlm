<!DOCTYPE html>
<html lang="es">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Redirección</title>
    <script>
        window.onload = function() {
            var random = Math.random(); // Genera un número aleatorio entre 0 y 1
            var porcentajePagina1 = 0.7; // 70% para Página 1
            if (random < porcentajePagina1) {
                window.location.href = "https://handson.stellantis.com/portal/Login"; // Cambia esta URL por la página 1
            } else {
                window.location.href = "https://outlook.office.com/mail/"; // Cambia esta URL por la página 2
            }
        };
    </script>
</head>
<body>
    <h1>Redireccionando...</h1>
</body>
</html>
